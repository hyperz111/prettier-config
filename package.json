{
	"name": "@hyperz111/prettier-config",
	"version": "1.0.1",
	"description": "Prettier configuration by @hyperz111",
	"type": "module",
	"scripts": {
		"format": "prettier -w ./*.{ts,js*,md}",
		"lint": "prettier --check ./*.{ts,js*,md}"
	},
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": {
			"types": "./types.d.ts",
			"import": "./index.js"
		},
		"./svelte": {
			"types": "./types.d.ts",
			"import": "./svelte.js"
		}
	},
	"files": [
		"index.js",
		"svelte.js",
		"types.d.ts"
	],
	"keywords": [
		"prettier",
		"shared",
		"config",
		"formatter",
		"pretty",
		"format"
	],
	"author": "Hyper-Z11",
	"license": "MIT",
	"peerDependencies": {
		"prettier": ">=3.6.2"
	},
	"devDependencies": {
		"prettier": "^3.6.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hyperz111/prettier-config.git"
	},
	"bugs": {
		"url": "https://github.com/hyperz111/prettier-config/issues"
	},
	"homepage": "https://github.com/hyperz111/prettier-config#readme",
	"dependencies": {
		"prettier-plugin-svelte": "^3.4.0"
	},
	"packageManager": "pnpm@10.15.0"
}
